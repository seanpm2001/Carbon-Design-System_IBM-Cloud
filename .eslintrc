{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "eslint-config-carbon/base"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "react-hooks",
      "jsx-a11y"
  ],
  "rules": {
    "curly": "off",
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error"
  }
}
