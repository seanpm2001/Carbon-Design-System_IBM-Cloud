{
  "name": "carbon-for-ibm-cloud",
  "version": "0.1.4",
  "main": "dist/index.js",
  "dependencies": {
    "classnames": "^2.3.2",
    "i18next": "^22.4.11",
    "i18next-browser-languagedetector": "^7.0.1",
    "react-i18next": "^12.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "webpack": "BABEL_ENV=production npx webpack",
    "build": "cross-env BABEL_ENV=production babel src -d dist && npm run copy_locales",
    "copy_locales": "cp -a src/locales/ dist/locales/",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -o docs-build -s public "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@carbon/react": "^1.16.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "prop-types": "^15.8.1",
    "sass": "^1.55.0",
    "source-map-explorer": "^2.5.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1"
  }
}
